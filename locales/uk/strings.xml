    <string name="pref_language">Мова</string>
    <!-- This is the name of the list preference with which the language can be set. -->
    <string name="pref_custom_language">Мова</string>
    <!-- This string marks the default language setting. -->
    <string name="locale_default">За змовчуванням</string>
