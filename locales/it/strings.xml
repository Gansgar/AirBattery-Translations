    <string name="pref_automatic_open">Chiudi e apri automaticamente</string>
    <string name="pref_automatic_open_summary">Non appena le AirPods / Beats sono (dis)connesse</string>
    <!-- This is the title of an option that, if enabled, hides the app from the recent tasks -->
    <string name="pref_hide_from_recent">Nascondi l\'app dalle app usate di recente</string>

    <string name="pref_auto_hide_title">Chiudi l\'app con un po\' di ritardo</string>


