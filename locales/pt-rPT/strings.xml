    <string name="system_overlay_permission_title">Permitir início do plano de fundo</string>
    <string name="system_overlay_permission_message">Para iniciar automaticamente, quando os AirPods / Beats estiverem conectados, é necessário permitir que este aplicativo seja iniciado em segundo plano.</string>

    <!-- Information Activities -->
    <!-- LOCALIZATION NOTE (explain_location_act_title, explain_location_act_message, explain_location_act_positive_redirect): Explain Location Activity -->
    <string name="explain_location_act_title">Autorização de localização</string>
    <string name="explain_location_act_message">O aplicativo só pode receber o status de seus fones de ouvido quando tiver acesso à sua localização.\n\nDesde o Android 6.0 somente aplicativos com a permissão acima podem acessar os pacotes de dados Bluetooth LE, como podem (entre outros) também ser usados para localizar telefone em um prédio.</string>
    <!-- When the user has enabled "always disable" the setting the activity will redirect to the settings. -->
    <string name="explain_location_act_positive_redirect">Abrir configurações</string>

    <!-- LOCALIZATION NOTE (device_selection_title, device_selection_message) This fragment allows you to select the devices you own. It's used to set the optimal settings for your setup -->
    <string name="device_selection_title">Selecione seus dispositivos</string>
    <string name="device_selection_message">Selecione todos os dispositivos que você usa. Isso adaptará a experiência do aplicativo à sua situação.</string>

    <!-- LOCALIZATION NOTE (energy_restriction_redirect, energy_restriction_positive) This is part of the first startup dialogs -->
    <string name="energy_restriction_redirect">Abrir configurações</string>
    <string name="energy_restriction_positive">Ignorar</string>

    <string name="request_location_redirect">Conceder permissão</string>

    <string name="huawei_deter_title">Seja cuidadoso!</string>
    <string name="huawei_deter_message">O fabricante do seu telefone é conhecido por produzir implementações Bluetooth com defeito. O suporte para a nova geração de fones de ouvido Apple / Beats pode ser limitado ou inexistente. Não há solução para isso.</string>

