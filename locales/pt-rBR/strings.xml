    <!-- This is a category that bundles the purchase things together. -->
    <string name="settings_purchases_title">Compras</string>
    <!-- (get-pro) -->
    <!-- When you press it the In-App-Purchase workflow by Google starts. -->
    <string name="settings_buy_pro_link_title">Doe para remover os anúncios e obter o pro</string>

    <string name="settings_buy_pro_trial_time">%1$d dias de tempo restante</string>

    <string name="settings_buy_airpods2">Compre o suporte do AirPods 2</string>

    <!-- IAP -->
    <string name="buy_pro">Por favor compre a licença pro primeiro.</string>
    <!-- A test that pops up, when the user has upgraded my app successfully. -->
    <string name="buy_pro_thanks">Obrigado por comprar o aplicativo.</string>
    <!-- LOCALIZATION NOTE (buy_pro_error_dialog_title, buy_pro_error_dialog_message):
