    <string name="array_pref_auto_hide_item_2">3 segundos</string>
    <string name="array_pref_auto_hide_item_3">5 segundos</string>
    <string name="array_pref_auto_hide_item_4">10 segundos</string>
    <string name="array_pref_auto_hide_item_5">30 segundos</string>

    <!-- Dialogs -->
    <!-- LOCALIZATION NOTE (custom_language_warning_dialog_title, custom_language_warning_dialog_message, custom_language_warning_dialog_positive):
          This dialog is shown after the user selected another language. It warns the user that the app needs to restart in order for this to work and offers this option.
          The user can also cancel the dialog, but this will not restore the selected language -->
    <!-- Title -->
    <string name="custom_language_warning_dialog_title">Aplicar cambios</string>
    <!-- Message -->
    <string name="custom_language_warning_dialog_message">Es necesario reiniciar la aplicación para aplicar los cambios.</string>
    <!-- The positive button -->
    <string name="custom_language_warning_dialog_positive">Reiniciar</string>

    <!-- LOCALIZATION NOTE (energy_measures_warning_dialog_title, energy_measures_warning_dialog_message, energy_measures_warning_dialog_positive):
          This dialog is shown, when the user sets the In-Ear-Detection button to true. It should make the user aware that the app works best if
          it is removed from any energy saving measures, power monitors, … and that he shouldn't write to me for that. Every phone is different in this aspect
          and I don't want to make a Google request for everybody for them. -->
    <!-- Title -->
    <string name="energy_measures_warning_dialog_title">Deshabilitar el ahorro de energía</string>
    <!-- Message -->
    <string name="energy_measures_warning_dialog_message">Algunos teléfonos pueden cerrar la aplicación en segundo plano. Si quieres la mejor experiencia, elimina esta aplicación de las opciones de ahorro de energía. Puedes buscar en internet como hacerlo.</string>
    <!-- Positive button -->
    <string name="energy_measures_warning_dialog_positive">Entendido</string>

    <!-- LOCALIZATION NOTE (gps_disabled_warning_title, gps_disabled_warning_message):
      When the location services are disabled the app ask for activating them. Please insert for location services the name on Android used in your language. (e.g. in German it's GPS services)-->
    <string name="gps_disabled_warning_title">Habilitar servicios de ubicación</string>
    <string name="gps_disabled_warning_message">Parece que tienes desactivados los servicios de ubicación. ¿Quieres que los activemos?</string>
    <!--!gps_disabled_warning_positive=@android:string/yes-->
    <!--!gps_disabled_warning_negative=@android:string/no-->

    <string name="gps_disabled_warning_negative">Ignorar para siempre</string>

    <!-- LOCALIZATION NOTE ( -->
    <string name="system_overlay_permission_title">Permitir inicio desde el fondo</string>
    <string name="system_overlay_permission_message">Para iniciar automáticamente, cuando AirPods / Beats están conectados, esta aplicación debe poder iniciarse desde el fondo.</string>

    <!-- Information Activities -->
    <!-- LOCALIZATION NOTE (explain_location_act_title, explain_location_act_message, explain_location_act_positive_redirect): Explain Location Activity -->
    <string name="explain_location_act_title">Permisos de ubicación</string>
    <string name="explain_location_act_message">La aplicación únicamente puede conocer el estado de tus auriculares cuando tiene acceso a tu localización.\n\nDesde Android 6.0 sólo las aplicaciones con dicho permiso pueden acceder a los paquetes de datos de Bluetooth LE, dado que estos (entre otros) pueden ser usados para localizar el teléfono dentro de un edificio.</string>
    <!-- When the user has enabled "always disable" the setting the activity will redirect to the settings. -->
    <string name="explain_location_act_positive_redirect">Abrir opciones</string>

    <!-- LOCALIZATION NOTE (device_selection_title, device_selection_message) This fragment allows you to select the devices you own. It's used to set the optimal settings for your setup -->
    <string name="device_selection_title">Selecciona tus dispositivos</string>
    <string name="device_selection_message">Seleccione todos los dispositivos que usa. Esto adaptará la experiencia de la aplicación a su situación.</string>

    <!-- LOCALIZATION NOTE (energy_restriction_redirect, energy_restriction_positive) This is part of the first startup dialogs -->
    <string name="energy_restriction_redirect">Configuración abierta</string>
    <string name="energy_restriction_positive">Ignorar</string>

    <string name="request_location_redirect">Conceder permiso</string>

    <string name="huawei_deter_title">¡Ten cuidado!</string>
    <string name="huawei_deter_message">Se sabe que el fabricante de su teléfono produce implementaciones defectuosas de Bluetooth. El soporte para la nueva generación de auriculares Apple / Beats puede ser limitado o no existir. No hay solución para esto.</string>

    <!-- LOCALIZATION NOTE (using_airpods2_title, using_airpods2_message)
